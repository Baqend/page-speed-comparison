{
  "name": "PageSpeedComparison",
  "description": "",
  "version": "1.0.0",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config ./config/webpack.config.dev.js --inline --hot --open --host localhost",
    "build:dev": "webpack --config ./config/webpack.config.dev.js",
    "build:prod": "webpack --config ./config/webpack.config.prod.js --optimize-minimize",
    "build:staging": "webpack --config ./config/webpack.config.staging.js --optimize-minimize",
    "build:report": "webpack --config ./config/webpack.config.report.js --optimize-minimize",
    "deploy:prod": "npm run build:prod && baqend deploy --file-dir dist makefast",
    "deploy:staging": "npm run build:staging && baqend deploy --file-dir dist makefast-staging",
    "deploy:dev": "npm run build:dev && baqend deploy --file-dir dist makefast-dev",
    "deploy-code:dev": "baqend deploy --code makefast-dev",
    "deploy-code:staging": "baqend deploy --code makefast-staging",
    "deploy-code:prod": "baqend deploy --code makefast",
    "build-proxy": "cd makefast-proxy && node build.js",
    "schema-download:dev": "baqend schema download makefast-dev && baqend typings makefast-dev",
    "schema-upload:dev": "baqend schema upload makefast-dev",
    "schema-upload:staging": "baqend schema upload makefast-staging",
    "schema-upload:prod": "baqend schema upload makefast"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.36",
    "@types/jquery": "^3.2.15",
    "awesome-typescript-loader": "^3.2.3",
    "baqend-coding-standard": "^1.1.1",
    "baqend-webpack-plugin": "^1.0.2",
    "chai": "^4.1.2",
    "css-loader": "^0.28.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.17.0",
    "typescript": "^2.5.3",
    "url-loader": "^0.5.9",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "baqend": "^2.8.6",
    "bootstrap": "^3.3.7",
    "clean-webpack-plugin": "^0.1.17",
    "jquery": ">=2.2.0",
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "regexpu-core": "^4.1.3",
    "request": "^2.83.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  }
}
